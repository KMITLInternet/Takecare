<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.takecare"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FoodActivity"
            android:label="@string/title_activity_food" >
        </activity>
        <activity
            android:name=".VacineActivity"
            android:label="@string/title_activity_vacine" >
        </activity>
        <activity
            android:name=".MusicActivity"
            android:label="@string/title_activity_music" >
        </activity>
        <activity
            android:name=".DiseaseActivity"
            android:label="@string/title_activity_disease" >
        </activity>
        <activity android:name=".ClipMusic" >
        </activity>
        <activity
            android:name=".Disease_Detail"
            android:label="@string/title_activity_disease__detail" >
        </activity>
    </application>

</manifest>
